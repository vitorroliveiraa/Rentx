{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch", //attach - launch
      "name": "Rentx",
      "args": ["${workspaceFolder}/src/shared/infra/http/server.ts"],
      "runtimeArgs": [
        "--nolazy",
        "-r",
        "ts-node/register",
        "-r",
        "tsconfig-paths/register"
      ],
      "sourceMaps": true,
      "port": 3306,
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceRoot}",
      "console": "integratedTerminal",
      "protocol": "inspector"
    }
  ]
}
